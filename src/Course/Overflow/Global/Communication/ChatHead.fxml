<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<AnchorPane id="AnchorPane" fx:id="root" styleClass="chatBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Course.Overflow.Global.Communication.ChatHeadController">
   <children>
      <HBox layoutX="10.0" layoutY="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="7.0" AnchorPane.rightAnchor="7.0" AnchorPane.topAnchor="0.0">
         <children>
            <Circle fx:id="instPhoto" fill="DODGERBLUE" radius="40.0" stroke="BLACK" strokeType="INSIDE" />
            <VBox style="-fx-spacing: 0; -fx-alignment: center-left;">
               <children>
                  <Label fx:id="instName" styleClass="teacherName" text="Teacher Name" />
                  <Label fx:id="lastMessage" styleClass="lastMessage" text="Last Message" />
                  <Label fx:id="time" styleClass="lastMessageTime" text="Course Name" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@../Global.css" />
      <URL value="@Communication.css" />
   </stylesheets>
</AnchorPane>
