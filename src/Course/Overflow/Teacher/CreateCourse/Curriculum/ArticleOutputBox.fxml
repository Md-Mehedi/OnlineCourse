<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="ArticleOutputContainer" fx:id="container" styleClass="centerRightWidth" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Course.Overflow.Teacher.CreateCourse.Curriculum.ArticleOutputBoxController">
   <children>
      <VBox alignment="CENTER" spacing="30.0" style="-fx-alignment: center-left;" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="20.0">
         <children>
            <Label id="Title" fx:id="title" lineSpacing="10.0" styleClass="caption" textAlignment="JUSTIFY" wrapText="true" />
            <Label id="Article" fx:id="article" lineSpacing="10.0" styleClass="title" textAlignment="JUSTIFY" wrapText="true" />
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <Button id="Edit" fx:id="edit" mnemonicParsing="false" onMouseClicked="#mouseClicked" text="Edit">
                     <styleClass>
                        <String fx:value="minScaleHover" />
                        <String fx:value="cursorHand" />
                     </styleClass></Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../../../Global/Global.css" />
      <URL value="@Curriculum.css" />
   </stylesheets>
</AnchorPane>
